*   Installed SSC-Ados: fre, mkspline2, lvr2plot2 & bigtab(ssc install xy) *****
********************************************************************************

clear

version 14
set more off, perm

global d "[insert global]"

cd "[insert directory]"

********************************************************************************

capture log close
log using exam_task_an1.log , replace

********************************************************************************
* Data - Soc. Strat. in Eastern Europe after 1989 for Czechoslovakia ***********
********************************************************************************

do $d/cr_fwilhelm92_task1.do

********************************************************************************
************************************ Task A ************************************
********************************************************************************

reg ln_hhincome i.n_party c.age1 c.age2
est sto m1

display exp(.142755)		// 15%
display exp(.0075873)		// .8%
display exp(.0075873*10)	// 8%
display exp(-.0312087)		// 1-.97 = -3%
display exp(-.0312087*10)	// 1-.73 = -27%
display exp(8.981745)		// 7956.5041

display exp(.1139441)		// 12.1%
display exp(.1715658)		// 18.7%

********************************************************************************
************************************ Task B ************************************
********************************************************************************

do $d/diag_fwilhelm92_task1.do
esttab m1 m2, p stats (N r2) star (* 0.05) 

********************************************************************************
************************************ Task C ************************************
********************************************************************************

reg ln_hhincome i.n_party c.age1 c.age2 i.n_educ i.n_speduc, base
est sto m3

test 1.n_educ 2.n_educ 3.n_educ
test 1.n_speduc 2.n_speduc 3.n_speduc

display exp(.0443305)		// 4.5%
display exp(.0134049)		// 1.4%
display exp(.0134049*10)	// 14.3%
display exp(-.0236661)		// 1-.98 = -2%
display exp(-.0236661*10)	// 1-.79 = -21%
display exp(.0555318)		// 5.7%
display exp(.1590824)		// 17.2%
display exp(.290912)		// 33.8%
display exp(.0672604)		// 7%
display exp(.1412297)		// 15%
display exp(.2019097)		// 22%
display exp(8.912522)		// 7424.3617

esttab m1 m3, p stats (N r2) star (* 0.05)



********************************************************************************
************************************ Task D ************************************
********************************************************************************

reg ln_hhincome i.n_party c.age1 c.age2 i.m_educ i.f_educ, base
est sto m4

display exp(.0488889)		// 5%
